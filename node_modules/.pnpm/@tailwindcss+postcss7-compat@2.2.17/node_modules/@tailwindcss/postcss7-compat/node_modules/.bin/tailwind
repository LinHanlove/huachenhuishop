#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/postcss7-compat/lib/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/postcss7-compat/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/postcss7-compat/lib/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/postcss7-compat/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules/@tailwindcss/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/@tailwindcss+postcss7-compat@2.2.17/node_modules:/mnt/c/Users/LinHan/Desktop/uni-app/huachenghuiShop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
